{
  "private": true,
  "name": "template-ts",
  "version": "1.0.0",
  "description": "Default template for TypeScript projects",
  "scripts": {
    "build": "esbuild src/index.ts --platform=node --bundle --outdir=dist",
    "lint": "eslint src/ --ext .ts",
    "test": "jest",
    "tsc": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "1.2.246",
    "@swc/jest": "0.2.22",
    "@types/jest": "29.0.0",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "esbuild": "0.15.6",
    "eslint": "8.23.0",
    "jest": "29.0.2",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "prettier": {
    "printWidth": 80
  }
}
